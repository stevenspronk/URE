<mvc:View controllerName="MVC.Dashboard" height="100%" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="MVC.Dashboard"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.layout="sap.ui.layout"
	xmlns:ui.commons="sap.suite.ui.commons" xmlns="sap.m">
	<Page showFooter="false" showHeader="false">
		<TileContainer allowAdd="false" busy="false" editable="false" height="100%" id="__container0">
			<tiles>
				<CustomTile class="sapUiMediumMarginBeginEnd sapUiTinyMarginBottom sapMTile" xmlns="sap.m">
					<content>
						<html:div xmlns:sap.ui.core="sap.ui.core">
							<sap.ui.core:Icon class="dashboardIcon" size="30pt" src="sap-icon://customer"/>
							<html:div class="dashboardTitle">Vermogen</html:div>
							<html:div class="info_text_tile" id="descriptionDiv">
								<html:div class="counter_home" id="counterDiv">
									<ui.commons:NumericContent class="sapUiSmallMargin" id="gearIndicator" indicator="Up" scale="" size="M" state="Loaded" value="{/GEAR}" valueColor="Neutral" withMargin="false"/>
								</html:div>
							</html:div>
							<html:content></html:content>
						</html:div>
					</content>
				</CustomTile>
				<CustomTile class="sapUiMediumMarginBeginEnd sapUiTinyMarginBottom sapMTile" xmlns="sap.m">
					<content>
						<html:div xmlns:sap.ui.core="sap.ui.core">
							<sap.ui.core:Icon class="dashboardIcon" size="30pt" src="sap-icon://connected"/>
							<html:div class="info_text_tile" id="descriptionDiv">Temperatuur per cel</html:div>
							<html:div>
								<html:div class="counter_home" id="counterDiv">
									<ui.commons:ColumnMicroChart class="sapUiSmallMargin" height="100px" press="press" size="S" width="80%">
										<ui.commons:columns>
											<ui.commons:ColumnData color="Good" height="5px" value="1" width="7px"/>
											<ui.commons:ColumnData color="Good" height="5px" value="20" width="7px"/>
											<ui.commons:ColumnData color="Good" height="5px" value="10" width="7px"/>
											<ui.commons:ColumnData color="Neutral" height="5px" value="30" width="7px"/>
											<ui.commons:ColumnData color="Good" height="7px" value="4" width="7px"/>
											<ui.commons:ColumnData color="Good" height="5px" value="80" width="7px"/>
										</ui.commons:columns>
									</ui.commons:ColumnMicroChart>
								</html:div>
							</html:div>
						</html:div>
					</content>
				</CustomTile>
				<CustomTile class="sapUiMediumMarginBeginEnd sapUiTinyMarginBottom sapMTile" xmlns="sap.m">
					<content>
						<html:div class="container">
							<sap.ui.core:Icon size="30pt" src="sap-icon://connected"/>
							<html:div class="info_text_tile" id="descriptionDiv">Status of charge</html:div>
							<html:div class="counter_home" id="counterDiv">
								<ui.commons:ColumnMicroChart class="sapUiSmallMargin" press="press" size="L" width="20%">
									<ui.commons:columns>
										<ui.commons:ColumnData color="Good" value="100"/>
									</ui.commons:columns>
								</ui.commons:ColumnMicroChart>
							</html:div>
						</html:div>
					</content>
				</CustomTile>
				<CustomTile class="sapUiMediumMarginBeginEnd sapUiTinyMarginBottom sapMTile" xmlns="sap.m">
					<content>
						<html:div class="container">
							<sap.ui.core:Icon size="30pt" src="sap-icon://connected"/>
							<html:div class="info_text_tile" id="descriptionDiv">Powerlimiet</html:div>
							<html:div class="counter_home" id="counterDiv">
								<ui.commons:ColumnMicroChart class="sapUiSmallMargin" press="press" size="L" width="20%">
									<ui.commons:columns>
										<ui.commons:ColumnData color="Good" value="100"/>
									</ui.commons:columns>
								</ui.commons:ColumnMicroChart>
							</html:div>
						</html:div>
					</content>
				</CustomTile>
				<StandardTile activeIcon="sap-icon://customer" class="sapUiMediumMarginBeginEnd sapUiTinyMarginBottom" icon="sap-icon://connected"
					id="__tile4" info="Info" infoState="Success" number="123" numberUnit="eur" removable="false" title="Title 1" type="None"/>
				<CustomTile class="sapUiMediumMarginBeginEnd sapUiTinyMarginBottom sapMTile" xmlns="sap.m">
					<content>
						<html:div class="container">
							<sap.ui.core:Icon size="30pt" src="sap-icon://connected"/>
							<html:div class="info_text_tile" id="descriptionDiv">Powerlimiet</html:div>
							<html:div class="counter_home" id="counterDiv">
								<ui.commons:ColumnMicroChart class="sapUiSmallMargin" press="press" size="L" width="20%">
									<ui.commons:columns>
										<ui.commons:ColumnData color="Good" value="100"/>
									</ui.commons:columns>
								</ui.commons:ColumnMicroChart>
							</html:div>
						</html:div>
					</content>
				</CustomTile>
				<StandardTile activeIcon="sap-icon://customer" class="sapUiMediumMarginBeginEnd sapUiTinyMarginBottom"
					icon="sap-icon://energy-saving-lightbulb" iconDensityAware="false" id="__tile1" info="Info" infoState="None" number="123" numberUnit="eur"
					removable="false" title="Title 2" type="None"/>
			</tiles>
		</TileContainer>
		
		<List class="sapUiResponsiveMargin" id="UreList" items="{ComponentTest&gt;/ZURE_ALL}" width="auto">
			<items>
				<ObjectListItem number="{ComponentTest&gt;THROTTLE}" title="{ComponentTest&gt;RACE_ID}"/>
			</items>
		</List>
	</Page>
</mvc:View>