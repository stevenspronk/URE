<mvc:View height="100%" width="100%"
	controllerName="MVC.Driver"
    xmlns:l="sap.ui.layout" 
    xmlns:viz="sap.viz.ui5.controls" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:suite="sap.suite.ui.commons"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
	>

	<l:FixFlex id="idFixFlex" class="fixFlexFixedSize" minFlexSize="250"> 
        <l:fixContent>    
            <Panel id="PN-1" class="panelStyle" expandable="true" expanded="false" headerText="Settings">
                <ScrollContainer 
                    class="scrollStyle"
                    height="0%" 
                    width="0%" 
                    horizontal="true"
                    vertical="false"
                    focusable="false">
                    <HBox class="settingBottomStyle" width="auto">
                        <HBox id="HB-1">
                            <HBox width="600px">
                                <VBox id="BX-1" class="boxPosition" width="350px">
                                    <Label text="Levels" class="sapUiSmallMarginTopBottom" design="Bold"/> 
                                     <Select id="timeAxisExampleSelect" class="selectPosition" autoAdjustHeight="false" maxWidth="300px">
                                       <items><core:Item key="1" text="Second / Minute"/></items>
                                       <items><core:Item key="2" text="Second / Minute / Lap"/></items>
                                       <items><core:Item key="3" text="Second / Minute / Lap / Race"/></items>
                                     </Select>
                                </VBox>
                                 <VBox id="BX-2" class="boxPosition" width="250px">
                                    <Label text="Chart Type" class="sapUiSmallMarginTopBottom" design="Bold"/> 
                                    <Select id="chartTypeSelect" selectedKey="3" class="selectPosition" autoAdjustHeight="false" maxWidth="200px">
<!--                                       <items><core:Item key="1" text="Bubble Chart"/></items>
                                       <items><core:Item key="2" text="Column Chart"/></items>
                                       <items><core:Item key="3" text="Line Chart"/></items>
-->                                       <items><core:Item key="4" text="Line Chart with Dynamic Value Axis"/></items>
<!--                                        <items><core:Item key="5" text="Scatter Chart"/></items> --> 
                                     </Select>
                                </VBox>
                            </HBox>
                        </HBox>
                    </HBox>
                </ScrollContainer>
            </Panel>
        </l:fixContent>

        <l:flexContent>
            <viz:Popover id="idPopOver"></viz:Popover>
            <viz:VizFrame id="idVizFrameTimeAxis" uiConfig="{applicationSet:'fiori'}" height="50%" width="100%"></viz:VizFrame>
        </l:flexContent>


	</l:FixFlex>
</mvc:View>