<mvc:View controllerName="MVC.LiveData" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	xmlns:smoothie="com.URE.Controls" xmlns="sap.m" xmlns:microchart="sap.suite.ui.microchart">
	<GenericTile header="Vermogen" size="M" frameType="OneByOne">
		<tileContent>
			<TileContent footer="">
				<content>
					<FlexBox height="100%" alignItems="Center" justifyContent="Center">
						<items>
							<Text textAlign="Center" width="100%" height="100%"
								text="{parts:[{path:'Overview>/d/results/0/BATTERY_CURRENT'},{path:'Overview&gt;/d/results/0/BATTERY_VOLTAGE'}], formatter:'.calculatePower'} W"
								wrapping="true" id="dashboardTilePowerText"/>
						</items>
					</FlexBox>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
	<GenericTile header="Temperatuur per cel" subheader="" frameType="SixByOne">
		<tileContent>
			<TileContent footer="">
				<content>
					<microchart:RadialMicroChart percentage="10" valueColor="Good"/>
					<microchart:RadialMicroChart percentage="20" valueColor="Error"/>
					<microchart:RadialMicroChart percentage="10" valueColor="Good"/>
					<microchart:RadialMicroChart percentage="20" valueColor="Error"/>
					<microchart:RadialMicroChart percentage="10" valueColor="Good"/>
					<microchart:RadialMicroChart percentage="20" valueColor="Error"/>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
