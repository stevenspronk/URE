<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns:smoothie="com.URE.Controls" xmlns="sap.m"
	xmlns:microchart="sap.suite.ui.microchart" xmlns:layout="sap.ui.layout" xmlns:f="sap.ui.layout.form" controllerName="MVC.LiveData">
	<!--<f:SimpleForm id="pedals" minWidth="1024" maxContainerCols="1" editable="false" layout="ResponsiveGridLayout" title="Stand van pedalen" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="1" columnsM="1">-->
