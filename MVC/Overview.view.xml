<mvc:View xmlns:mvc="MVC.Overview" xmlns="sap.m" xmlns:core.mvc="sap.ui.core.mvc" controllerName="MVC.Overview">
	<Page title="URE Racing: Overview" class="sapUiContentPadding" id="_appView" showNavButton="true" navButtonPress="onNavBack">
		<IconTabBar id="idIconTabBarNoIcons" stretchContentHeight="true" expanded="true" expandable="false" class="sapUiResponsiveContentPadding" select="onSelect" backgroundDesign="Transparent">
			<items>
				<IconTabFilter key="Dashboard" text="Overview">
					<core.mvc:XMLView viewName="MVC.Dashboard"/>
				</IconTabFilter>
				<IconTabFilter key="Car" text="Auto">
					<core.mvc:XMLView viewName="MVC.Car"/>
				</IconTabFilter>
				<IconTabFilter key="Driver" text="Coureur" press="onNavBack">
					<core.mvc:XMLView viewName="MVC.Driver"/>
				</IconTabFilter>
				<IconTabFilter key="Powertrain" text="Powertrain">
					<core.mvc:XMLView viewName="MVC.Powertrain"/>
				</IconTabFilter>
			</items>
		</IconTabBar>
		<footer>
			<Toolbar>
				<ToolbarSpacer/>
				<Button text="Run" width="100px" id="_newRunBtn" activeIcon="sap-icon://add" icon="sap-icon://add"/>
				<Button text="Stop Test" activeIcon="sap-icon://stop" press="newTest" icon="sap-icon://stop" id="_newTestBtn"/>
			<content/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>