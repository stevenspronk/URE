<mvc:View xmlns:mvc="MVC.Overview" xmlns="sap.m" xmlns:core.mvc="sap.ui.core.mvc" controllerName="MVC.Overview">
	<Page title="URE Racing: Overview" class="sapUiContentPadding" id="_appView" navButtonPress="onNavBack" showNavButton="false">
		<IconTabBar id="idIconTabBarNoIcons" stretchContentHeight="true" expanded="true" expandable="false" class="sapUiResponsiveContentPadding" select="onSelect" backgroundDesign="Transparent">
			<items>
				<IconTabFilter key="Dashboard" text="Overview" id="OverviewTab">
					<core.mvc:XMLView viewName="MVC.Dashboard" id="OverviewElement"/>
				</IconTabFilter>
				<IconTabFilter key="Car" text="Auto">
					<core.mvc:XMLView viewName="MVC.Car"/>
				</IconTabFilter>
				<IconTabFilter key="Driver" text="Coureur" press="onNavBack">
					<core.mvc:XMLView viewName="MVC.Driver"/>
				</IconTabFilter>
				<IconTabFilter key="Powertrain" text="Powertrain">
					<core.mvc:XMLView viewName="MVC.Powertrain"/>
				</IconTabFilter>
				<IconTabFilter key="Live" text="Live">
					<core.mvc:XMLView viewName="MVC.LiveData"/>
				</IconTabFilter>
			</items>
		</IconTabBar> 
		<footer>
			<Toolbar>
			<content>
			    <Label text="{ID>/raceID} / {ID>/runID}" width="100%" id="raceIdentify" visible="true"/>
			</content>
			    <ToolbarSpacer/>
				<Button text="Run" width="100px" id="_newRunBtn" activeIcon="sap-icon://add" icon="sap-icon://add" press="newRun"/>
				<Button text="Stop Test" activeIcon="sap-icon://stop" press="newTest" icon="sap-icon://stop" id="_newTestBtn"/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>