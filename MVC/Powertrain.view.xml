<mvc:View controllerName="MVC.Powertrain" height="100%" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="MVC.Powertrain"
	xmlns:sap.ui.core="sap.ui.core" xmlns:sap.ui.layout.form="sap.ui.layout.form" xmlns:sap.ui.layout="sap.ui.layout"
	xmlns:ui.commons="sap.suite.ui.commons" xmlns="sap.m">
	<Panel>
		<Label id="RaceIdLabel" text="{RaceMetaData>/RaceId}"></Label>


		<List id="Msg" items="{Msg>/MESSAGES}" width="100%">
			<items>
				<ObjectListItem id="MsgIt" number="{Msg>TIMESTAMP}" title="{Msg>MSG_TEXT}"></ObjectListItem>
			</items>
		</List>
		
	</Panel>
</mvc:View>