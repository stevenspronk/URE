{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.URE",
		"type": "application",
		"i18n": "",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "com.URE.MVC.App",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {}
			}
		},
		"config": {
			"serviceConfig": {
				"name": "UreRaceData.OData.UreOdataService",
				"serviceUrl": "/destinations/McCoy_URE/UreOdataService.xsodata/"
			},
			"messageService": {
				"name": "UreRaceData.OData.Messages",
				"serviceUrl": "/destinations/McCoy_URE/Messages.xsodata/"
			}
		},
		"routing": {
			"config": {
				"routerClass" : "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "MVC",
				"targetControl": "navContainer",
				"targetAggregation": "pages",
				"clearTarget": false,
				"controlId": "navContainer",
				"viewLevel": 1,
				"controlAggregation": "pages"
			},
			"routes": [{
				"pattern": "",
				"name": "CreateTest",
				"view": "CreateTest",
				"greedy": false
			}, {
				"pattern": "Overview",
				"name": "Overview",
				"target": "Overview"
			}],
			"targets": {
				"Overview": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Overview"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.URE.i18n.i18n"
				}
			}
		},
		"contentDensities": {
			"compact": false,
			"cozy": false
		}
	}
}